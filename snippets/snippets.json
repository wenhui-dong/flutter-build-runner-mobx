{
	"MobX Store": {
		"prefix": "mobx",
		"body": [
			"import 'package:mobx/mobx.dart';",
			"part '${TM_FILENAME_BASE/(.*)/${1:/downcase}/}.g.dart';",
			"",
			"class ${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 = _${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 with _$${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1;",
			"",
			"abstract class _${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 with Store {",
			"\t${0}",
			"}"
		],
		"description": "Create MobX Store"
	},
	"MobX Observer": {
		"prefix": "obs",
		"body": [
			"Observer(builder: (_) {",
			"   return Container(${0});",
			"},",
			")"
		],
		"description": "Create Widget Observer"
	},
	"MobX Getter Setter": {
		"prefix": "obx",
		"body": [
			"@observable",
			"$1 $2 = $3;",
			"@action",
			"set${2/(.*)/${1:/capitalize}/}($1 value) => $2 = value;",
			""
		],
		"description": "Create MobX Getter Setter"
	},
	"MobX Model": {
		"prefix": "mobxm",
		"body": [
			"import 'package:mobx/mobx.dart';",
			"import 'package:json_annotation/json_annotation.dart';",
			"",
			"part '${TM_FILENAME_BASE/(.*)/${1:/downcase}/}.g.dart';",
			"",
			"@JsonSerializable()",
			"class ${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 extends _${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 with _$${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 {",
			"",
			"${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1();",
			"",
			"factory ${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1.fromJson(Map<String, dynamic> json) => _$${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1FromJson(json);",
			"Map<String, dynamic> toJson() => _$${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1ToJson(this);",
			"",
			"}",
			"",
			"abstract class _${TM_FILENAME_BASE/(?:^|[_\\s])(\\w)/${1:/upcase}/g}$1 with Store {",
			"\t${0}",
			"}"
		],
		"description": "Create model with MobX Store and JsonSerializable"
	}
}